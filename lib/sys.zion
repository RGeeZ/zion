module sys @0.1
link module posix

# TODO: make module variable initialization deterministic
var args [str]

def exit(code int)
	posix.exit(code.raw)

def set_args(argc __int__, argv *__str__)
	print("set_args sees " + str(argc))
	print(reserved(args))
	print(len(args))
	print(argv[0r])
	i := argc - 1r
	while i >= 0r
		vector.append(args, __box__(argv[i]))
		i = i - 1r
