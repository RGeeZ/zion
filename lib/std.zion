fn enumerate(f any) any {
    let iterator = __iter__(f)
    i := Ref(-1)
    return fn () {
        i.value += 1
        return match iterator() {
            Just(t) => Just((i.value, t))
            Nothing => Nothing
        }
    }
}

type IntRangeIter has {
    lim     int
    step    int
    var pos int
}

fn __iter__[T](f Iter T) Iter T => f

fn range(lim int) Iter int {
    return range(0, lim, 1)
}

fn range(first int, lim int) Iter int {
    return range(first, lim, 1)
}

fn range(first int, lim int, step int) Iter int {
    iter := IntRangeIter(lim, step, first)
    return fn () int? {
        if iter.pos < iter.lim {
            j := iter.pos
            iter.pos += iter.step
            return Just(j)
        } else {
            return Nothing
        }
    }
}

fn __box__[Args, Ret](f fn _(..Args) Ret) fn (..Args) Ret {
    return __closure_t(f) as! fn (..Args) Ret
}

fn __eq__(x (), y ()) true {
    return true
