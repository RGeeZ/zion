fn print[T where is_maybe T](x T) {
    if x != null {
        print(x)
    } else {
        write(stdout, "<null>\n")
    }
}

fn print[T, Args, R where not (T <: Maybe any) and not (is_maybe T) and not (T === str) and not (T <: fn (..Args) R)](x T) {
    write(stdout, str(x))
    write(stdout, "\n")
}

