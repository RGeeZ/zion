module list

type List T is
	Cons(t T, next List T)
	Nil

def nth[T](l List T, index int) T?
	while index >= 0
		match l
			Cons(t, next)
				if index == 0
					return Just(t)
				else
					l = next
					index -= 1
			Nil
				return Empty
	return Empty

[global]
def len(l List any) int
	match l
		Cons(_, next)
			return len(next) + 1
		Nil
			return 0

[global]
def str(l List any) str
	return "[...list...]"
