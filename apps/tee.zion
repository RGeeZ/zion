module tee

get sys
get file

def main()
	if len(sys.args) == 2
		newline := "\n"
		f := open(sys.args[1], "w")
		state := f.state
		when state
			is file.Open
				for line in readlines()
					print(line)
					write(f, line)
					write(f, newline)
				close(f)
			else
				print("Failed to open file " + sys.args[1])
				sys.exit(-1)
	else
		for line in readlines()
			print(line)
