module when_contact

type Golfer{T} has
	var first T
	var last T

type Athlete is Golfer{str, str}

def recognize_golfer(athlete Athlete)
	when athlete
		is Golfer("Arnold", "Palmer")
			print("RIP")
		is Golfer(var x, var x)
			print("golfer has the same first name as their last name?")
		is Golfer("Tiger", var last)
			print("probably Tiger Woods")
		is Golfer(var first, var last)
			print("i guess you are " + first + " " + last)
	else
		print("who are you?")

def main()
	recognize_golfer(Golfer("Arnold", "Woods"))
