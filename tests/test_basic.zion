fn sqr(a) => a * a

fn tuple(h) {
    return (h,)
}

data Maybe a {
    Nothing
    Just(a)
}

class Functor f {
    fn fmap(fn (a) b, f a) f b  
}

fn add(x, y) => x + y
fn twice(f, x) => f(f(x))
fn plus4(x) => twice(add(2))(x)
fn hey() {
    return plus4(3)
}

class Stringable a {
    fn str(a) str
}

data Ordering {
    EQ
    LT
    GT
}

class Num a {
    has Ord
    fn from_int(int) a
    fn +(a, a) a
    fn *(a, a) a
    fn -(a, a) a
    fn /(a, a) a
}

fn doit(m) {
    return m
}

fn test_doit() {
    return doit(Just("hey"))
}

fn dd(x) {
    return sqr(x)
}
fn main() {
    return fmap(fn (x) => "str", Just(3))
    # return dd("hey")
}
