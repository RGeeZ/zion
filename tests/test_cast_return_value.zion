module _
# test: pass
# expect: 3
# expect: 4.5

fn read(x str) int {
    return int(x)
}

fn read(x str) float {
    return float(x)
}

fn read(fp *posix.FILE) int {
    var x int = 0
    assert(posix.fscanf(fp, "%d", &x as! *void) == 1)
    return x
}

fn read(fp *posix.FILE) float {
    var x float = 0.0
    assert(posix.fscanf(fp, "%lf", &x as! *void) == 1)
    return x
}

fn main() {
    print(read("3") as int)
    print(read("4.5") as float)
}
