def read(fp *posix.FILE) int {
    var x int = 0
    assert(posix.fscanf(fp, "%d",  &x as! *void) == 1)
    return x
}

def read(fp *posix.FILE) float {
    var x float = 0.0
    assert(posix.fscanf(fp, "%lf",  &x as! *void) == 1)
    return x
}

def main() {
    print("Gimme an int: ")
    print(read(stdin) as int)
    print("Gimme a float: ")
    print(read(stdin) as float)
}
