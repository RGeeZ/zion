# expect: 28231

def nth(l (Node any T)?, i int) any T?
	if l == null
		return null

	if i == 0
		return l.value
	else
		return nth(l.next, i - 1)

type Node T has
	var value T
	var next (Node T)?

def main()
	x := Node(3, Node(28231, null))
	y := nth(x, 1)
	if y != null
		print(y)
