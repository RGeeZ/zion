module _
link module file

def main() __int__
	stdout := (1).raw
	with f := file.open("play/read_file.zion", file.ReadOnly)
		if f
			while true
				buffer := file.read(f, 4 * 1024)
				when buffer
					is file/EOF
						break
					is std/bytes
						posix.write(stdout, buffer.data, buffer.cb)
					is file/ReadError
						print("Read Error")
						return (-1).raw
	return (0).raw
