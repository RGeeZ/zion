module _
get file
get os
get sys

# test: pass
# expect: This text should appear

fn main() {
    with fp := file.open(__filename__, "r") {
        while true {
            match readline(fp) {
                Line(line) => print(line)
                EOF        => break
                Error(_)   => break
            }
        }
    } else errno {
        print("Failed to open " + __filename__ + " with error " + errno)
    }
}
