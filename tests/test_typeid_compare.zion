module _
# test: pass

tag Cow
tag Hiss
type Boo is Hiss or Cow

# Note that this function returns an int, because we don't yet
# support creating bools
def test(a any, b any) bool
	return typeid(a) != typeid(b)

def main()
	i := Cow
	j := Hiss
	if test(i, j)
		print("different types")
	else
		print("same types")
