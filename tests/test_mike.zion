type Expr is
	Plus(lhs Expr, rhs Expr)
	Times(lhs Expr, rhs Expr)
	Value(x int)

def eval(expr Expr) int
	when expr is
		Plus(lhs, rhs)
			return eval(lhs) + eval(rhs)
		Times(lhs, rhs)
			return eval(lhs) * eval(rhs)
		Value(x)
			return x

def main()
	let e Expr = Value(3)
