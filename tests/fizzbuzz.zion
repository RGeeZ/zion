module fizzbuzz @0.1.0
# test: pass

fn main()
    var maximum int = 10
    if max_arg := nth(args, 1)
        maximum = int(max_arg)

    var i int = 1
    while i <= maximum
        if i % 15 == 0
            print("fizzbuzz")
        elif i % 3 == 0
            print("fizz")
        elif i % 5 == 0
            print("buzz")
        else
            print(i)
        i = i + 1
