module _

def truthy(x any) bool
	# error: when statements only work with managed types
	when x
		is int 
			print("int found")
			return x != 0
		is float
			print("float found")
			return x != 0
		is bool
			print("bool found")
			return x

def main()
	y := 3.33
	truthy(y)
	truthy(false)
	truthy(23)
