module _
# test: pass
# expect: int found
# expect: float found
# expect: bool found

def truthy(x any) bool
	when x
		is int 
			print("int found")
			return x != 0
		is float
			print("float found")
			return x != 0
		is bool
			print("bool found")
			return x

def main()
	y := 3.33
	truthy(y)
	truthy(false)
	truthy(23)
