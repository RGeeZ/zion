get itertools {take, chain, repeat, cycle, izip}

fn main() {
    for x in take(6, chain([
            repeat("a", 3),
            cycle(["a", "b", "c", "d"])
        ]))
    {
        print(x)
    }
    for (x, y) in izip(iter(["a", "b", "c"]), iter(repeat(4))) {
        print("(" + str(x) + ", " + str(y) + ")")
    }
    var y = ""
    for x in chain([["5", "6", "7"], ["8", "9", "0"]]) {
        y += x
    }
    print(y)
}
